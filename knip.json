{
  "$schema": "https://raw.githubusercontent.com/webpro/knip/main/schema.json",
  "entry": ["src/app/**/*.{ts,tsx}", "src/pages/**/*.{ts,tsx}"],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": ["src/components/ui/**"],
  "ignoreDependencies": ["@radix-ui/react-label"],
  "ignoreBinaries": [],
  "rules": {
    "exports": "error",
    "types": "error",
    "files": "warn",
    "dependencies": "warn"
  }
}
