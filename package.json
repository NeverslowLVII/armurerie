{
  "name": "armurerie",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development NODE_NO_WARNINGS=1 next dev",
    "turbo:dev": "turbo dev",
    "build": "prisma generate && next build",
    "turbo:build": "turbo build",
    "start": "next start",
    "db:push": "dotenv -e .env.development -- prisma db push",
    "db:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "db:seed": "dotenv -e .env.development -- prisma db seed",
    "db:validate": "prisma format && prisma validate",
    "lint": "next lint",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "check-types": "tsc --noEmit",
    "find-duplicates": "jscpd ./src",
    "find-unused": "ts-prune -p tsconfig.json",
    "check-deps": "depcheck",
    "knip": "knip",
    "biome:check": "biome check .",
    "biome:format": "biome format . --write",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:bundle": "source-map-explorer .next/static/**/*.js",
    "bundlesize": "bundlesize",
    "update-deps": "ncu",
    "update-deps:upgrade": "ncu -u",
    "clean": "npm run lint:fix && npm run format",
    "clean:deps": "rimraf node_modules/.cache",
    "clean:build": "rimraf .next",
    "security": "snyk test",
    "security:fix": "snyk wizard",
    "a11y": "pa11y-ci http://localhost:3000",
    "a11y:report": "pa11y-ci --reporter csv http://localhost:3000 > reports/accessibility.csv",
    "lighthouse": "lhci autorun",
    "optimize-image": "imagemin public/images/* --out-dir=public/images",
    "pre-deploy": "npm run validate && npm run clean && npm run find-unused && npm run build",
    "pre-deploy:check": "node ./scripts/pre-deploy-check.js",
    "prepare": "husky && husky install",
    "full-check": "npm run validate && npm run security && npm run find-unused && npm run a11y"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@internationalized/date": "^3.7.0",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.5",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-hover-card": "^1.1.5",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.5",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.5",
    "@radix-ui/react-toggle": "^1.1.1",
    "@radix-ui/react-toggle-group": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@reduxjs/toolkit": "^2.5.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/next-auth": "^3.15.0",
    "@types/nodemailer": "^6.4.17",
    "@vercel/blob": "^0.27.2",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.18.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.471.1",
    "next": "^14.0.4",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.4",
    "nodemailer": "^6.9.16",
    "react": "^18.2.0",
    "react-aria-components": "^1.6.0",
    "react-day-picker": "^9.5.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-parallax-tilt": "^1.7.282",
    "react-redux": "^9.2.0",
    "recharts": "^2.15.0",
    "sonner": "^1.7.2",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@lhci/cli": "^0.14.0",
    "@next/bundle-analyzer": "^15.3.1",
    "@prisma/client": "^6.3.1",
    "@types/node": "^20.17.19",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "bundlesize": "^0.18.2",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^8.0.3",
    "imagemin-cli": "^8.0.0",
    "jscpd": "^4.0.5",
    "knip": "^5.50.5",
    "lint-staged": "^15.5.1",
    "npm-check-updates": "^17.1.18",
    "pa11y": "^8.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.3.1",
    "rimraf": "^6.0.1",
    "snyk": "^1.1296.2",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.6",
    "ts-prune": "^0.10.3",
    "turbo": "^2.5.0",
    "typescript": "^5.3.3"
  }
}
